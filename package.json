{
  "name": "medica-scientific-parser",
  "version": "1.0.0",
  "description": "Excel parser for Medica Scientific simulation data - converts simulation output to normalized JSON",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/backend/server.js",
    "build:backend": "tsc -p tsconfig.backend.json",
    "server": "npm run build:backend && node dist/backend/server.js",
    "dev": "tsx watch backend/server.ts",
    "dev:frontend": "vite",
    "build": "npm run build:backend",
    "build:frontend": "vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "excel",
    "parser",
    "simulation",
    "medica-scientific",
    "data-ingestion"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chart.js": "^4.4.3",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.10",
    "@vitejs/plugin-react": "^4.3.1",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.0"
  }
}
